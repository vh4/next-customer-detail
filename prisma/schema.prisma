generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Transaksi {
  id              Int      @id @default(autoincrement())
  productID       String
  productName     String
  amount          String
  customerName    String
  status          Int
  transactionDate DateTime @default(now())
  createBy        String 
  createOn        DateTime @default(now())
}




